{
  "printWidth": 80,           // ✓ Déjà bon
  "tabWidth": 2,              // ✓ Déjà bon
  "useTabs": false,           // ✓ Déjà bon (espaces)
  "semi": true,               // ❌ Point-virgule obligatoire
